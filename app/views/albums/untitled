    <li role="presentation"><a role="menuitem" tabindex="-1" href="#{destroy_album_path(Family.find(params[:id]),@album)}", method="delete">Delete</a></li>
    <li role="presentation"><a role="menuitem" tabindex="-1" href="#{albums_path(Family.find(params[:id]),@album)}">All Albums</a></li>
    <li role="presentation" class="divider"></li>
    <li role="presentation">
