<div class="message_all">
  <%= @conversation.title %>
  <% @conversation.messages.each do |message| %>
    <%= render "show_message", {message: message} %>
  <% end %>
</div>


<%= form_for [@family, @conversation, @message], remote: true do |f| %>
<%= f.label :content %>
<%= f.text_field :content %>

<%= f.submit %>
<% end %>
